{
  "compilerOptions": {
    "module": "ES6",
    "checkJs": true,
    "removeComments": false,
    "declaration": true,
    "target":"ES6",
    "pretty": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./build"
  },
  "include":["./src/**/*"],
  "exclude": ["node_modules"]
}
